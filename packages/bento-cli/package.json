{
	"name": "@tamagui/bento-cli",
	"version": "1.98.0",
	"license": "MIT",
	"bin": "dist/cli.js",
	"type": "module",
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"build": "tsc --outDir dist --rootDir source",
		"dev": "tsc --watch",
		"test": "prettier --check . && xo && ava",
		"link": "npm link --local --force"
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"@inkjs/ui": "^1.0.0",
		"@supabase/supabase-js": "^2.43.1",
		"conf": "^12.0.0",
		"configstore": "^6.0.0",
		"copy-paste": "^1.5.3",
		"fuse.js": "^7.0.0",
		"got": "^14.2.1",
		"ink": "^4.1.0",
		"ink-link": "^3.0.0",
		"ink-text-input": "^5.0.1",
		"node-fetch": "^3.3.2",
		"node-localstorage": "^3.0.5",
		"octokit": "^4.0.2",
		"pastel": "^2.0.0",
		"react": "^18.2.0",
		"swr": "^2.2.5",
		"zod": "^3.21.4"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^3.0.1",
		"@types/copy-paste": "^1.1.33",
		"@types/react": "^18.0.32",
		"@vdemedes/prettier-config": "^2.0.1",
		"ava": "^5.2.0",
		"chalk": "^5.2.0",
		"eslint-config-xo-react": "^0.27.0",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"ink-testing-library": "^3.0.0",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.3",
		"xo": "^0.54.2"
	},
	"ava": {
		"extensions": {
			"ts": "module",
			"tsx": "module"
		},
		"nodeArguments": [
			"--loader=ts-node/esm"
		]
	},
	"xo": {
		"extends": "xo-react",
		"rules": {
			"react/prop-types": "off"
		}
	}
}
